option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: application
    NumProcesses: 1
    NumThreads: 15

container_commands:
  download:
    command: |
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download en_core_web_sm
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download en_core_web_md
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download en_core_web_lg
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download en_core_web_lg
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download en_core_web_trf
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m spacy download xx_ent_wiki_sm
      source /var/app/venv/staging-LQM1lest/bin/activate && python -m textblob.download_corpora
